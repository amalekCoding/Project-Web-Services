<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CurrencyConverterService WSDL service -->

  <service name="CurrencyConverter" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://currency.uge.fr"/>
      <parameter name="wsdlServiceElement" value="CurrencyConverterService"/>
      <parameter name="schemaQualified" value="http://currency.uge.fr"/>
      <parameter name="wsdlServicePort" value="CurrencyConverter"/>
      <parameter name="className" value="fr.uge.currency.CurrencyConverterSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="CurrencyConverter"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="convertEuroTo" qname="operNS:convertEuroTo" xmlns:operNS="http://currency.uge.fr" returnQName="retNS:convertEuroToReturn" xmlns:retNS="http://currency.uge.fr" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:currency" xmlns:pns="http://currency.uge.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:amount" xmlns:pns="http://currency.uge.fr" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="convertEuroTo"/>

  </service>
</deployment>
